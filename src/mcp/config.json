{
    "name": "jarvis",
    "version": "0.1.0",
    "description": "AI-powered ZSH plugin using MCP",
    "capabilities": {
        "command_processing": {
            "enabled": true,
            "models": ["gpt-4"]
        },
        "error_analysis": {
            "enabled": true,
            "models": ["gpt-4"]
        }
    },
    "settings": {
        "trigger_prefix": "@jarvis",
        "response_cache_enabled": true,
        "cache_ttl_seconds": 3600,
        "max_history_items": 100
    }
}
